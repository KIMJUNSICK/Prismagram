{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Cloning Instagram with GraphQL + Prisma + React + React Native + etc . . .",
  "main": "index.js",
  "repository": "https://github.com/KIMJUNSICK/Prismagram",
  "author": "Kim Jun Sik <jinaganda5@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "copy": "^0.3.2",
    "dotenv": "^8.0.0",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.5",
    "prisma-client-lib": "^1.34.1"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run generate",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy ./api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5"
  }
}
